(function(){$(function(){return setting_coding_area()}),this.setting_coding_area=function(){return window.editor=ace.edit("editor-div"),editor.setReadOnly(!0),editor.setOptions({maxLines:Infinity}),editor.setOptions({minLines:20}),editor.setOptions({enableBasicAutocompletion:!0,enableLiveAutocompletion:!0,enableSnippets:!0}),editor.setTheme(get_init_editor_theme()),editor.setFontSize(get_init_editor_font_size()),editor.insert("// \u81ea\u5206\u306e\u5f97\u610f\u306a\u8a00\u8a9e\u3067\n// Let's \u30c1\u30e3\u30ec\u30f3\u30b8\uff01\uff01"),$("select#language_id.box").change(function(){return ace_editor_change_language($(this).val(),editor,["\u81ea\u5206\u306e\u5f97\u610f\u306a\u8a00\u8a9e\u3067","Let's \u30c1\u30e3\u30ec\u30f3\u30b8\uff01\uff01"])})}}).call(this);