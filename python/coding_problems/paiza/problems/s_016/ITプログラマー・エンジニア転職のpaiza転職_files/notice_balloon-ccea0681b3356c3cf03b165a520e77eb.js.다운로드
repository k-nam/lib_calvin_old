(function(){var n,t;$(function(){return $(".notice-balloon:hidden").each(function(){if(t(this))return n(this)}),$(window).scroll(function(){return $(".notice-balloon:hidden").each(function(){if(t(this))return n(this)})})}),t=function(n){var t,o,i;return!!$(n).offset()&&(i=$(window).height(),o=$(window).scrollTop(),t=$(n).parent().offset().top,o>t-i+30)},n=function(n){return $(n).show("slow")}}).call(this);