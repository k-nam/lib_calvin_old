(function(){var n,t;$(function(){var a,s;return a=function(n){return"0"===n?t():$(".time_box").countdown({until:n,onExpiry:t,layout:'\u3042\u3068<span class="num">{hn}</span>{hl}<span class="num">{mn}</span>{ml}<span class="num">{sn}</span>{sl}'})},s="/challenges/"+$("#problem_id").val()+"/timer",n(s,a)}),n=function(n,t){return $.ajax({url:n,type:"GET",dataType:"text",error:function(){},success:t})},t=function(){return $(".time_box").html("\u30bf\u30a4\u30e0\u30a2\u30c3\u30d7\uff01")}}).call(this);