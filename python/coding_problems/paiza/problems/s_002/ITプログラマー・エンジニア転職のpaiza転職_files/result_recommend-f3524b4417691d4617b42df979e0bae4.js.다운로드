(function(){var e,o;this.initialize_result_recommend=function(){return e(),o()},e=function(){return $("div.offer_box").click(function(e){$(e.target).is("a")||$(e.target).is(".list-bookmark-img")||(window.recommend_clicked||(window.recommend_clicked=!0,gaq_push(["_trackEvent","Challenge Result Recommend JobOffers","Goto JobOffer Detail"])),window.open($(this).find("a").eq(0).attr("href"),"_blank"))})},o=function(){return $(".offer_box").hover(function(){if($(this).css("cursor","pointer"),$(".listWrap").css({backgroundColor:"transparent"}),$(this).find(".title a").addClass("active").css,$(this).css({backgroundColor:"#f5f5f5"}),!window.recommend_hovered)return window.recommend_hovered=!0,gaq_push(["_trackEvent","Challenge Result Recommend JobOffers","Hover"])},function(){return $(this).find(".title a").removeClass("active"),$(this).css({backgroundColor:"#ffffff"})})},this.tracking_bookmark=function(){if(!window.recommend_bookmarked)return window.recommend_bookmarked=!0,gaq_push(["_trackEvent","Challenge Result Recommend JobOffers","Bookmark"])}}).call(this);